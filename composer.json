{
    "name": "effsoft/wordpress-plugin-framework",
    "description": "",
    "keywords": ["wordpress","plugin","framework"],
    "homepage": "https://github.com/effsoft/wordpress-plugin-framework",
    "type": "framework",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
      "php": ">=7.2.0",
      "ext-json": "*",
      "marc-mabe/php-enum": "~v2.3.2",
      "ircmaxell/random-lib": "~1.2.0",
      "hashids/hashids": "~3.0.0",
      "guzzlehttp/guzzle": "~6.3.3",
      "smarty/smarty":"~3.1",
      "symfony/filesystem":"~5.0",
      "rlanvin/php-form": "2.*"
    },
    "require-dev": {
      
    },
    "autoload" : {
      "psr-4" : {
        "wordpress\\plugin\\" : "src/wordpress/plugin"
      }
    },
    "autoload-dev" : {
      "psr-4" : {
        "wordpress\\plugin\\" : "src/wordpress/plugin"
      }
    },
    "config": {
      "vendor-dir": "../vendor",
      "process-timeout": 1800,
      "fxp-asset": {
        "enabled": false
      }
    },
    "repositories": {
      "asset-packagist":{
        "type": "composer",
        "url": "https://asset-packagist.org"
      },
      "packagist":{
        "type": "composer",
        "url": "https://packagist.org"
      }
    }
  }